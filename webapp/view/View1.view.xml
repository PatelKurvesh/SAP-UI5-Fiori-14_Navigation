<mvc:View controllerName="GithubSAP-UI5-Fiori-14_Navigation.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<headerContent>
					<Button icon="sap-icon://action" press="onNext"></Button>
				</headerContent>
				<content>
					<List items="{/fruits}" itemPress="onItemPress">
						<ObjectListItem
						type="Navigation"
						title="{name}"
						number="{price}">
							<ObjectAttribute text="{color}"></ObjectAttribute>
							<ObjectAttribute text="{taste}"></ObjectAttribute>
							
						</ObjectListItem>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>